{
    "name": "cakedc/cakephp-phppm",
    "description": "PHP PM bridge for CakePHP",
    "type": "cakephp-plugin",
    "keywords": [
        "cakephp",
        "php",
        "pm"
    ],
    "homepage": "https://github.com/CakeDC/cakephp-phppm",
    "license": "MIT",
    "authors": [
        {
            "name": "CakeDC",
            "homepage": "https://www.cakedc.com",
            "role": "Author"
        },
        {
            "name": "Others",
            "homepage": "https://github.com/CakeDC/cakephp-phppm/graphs/contributors"
        }
    ],
    "support": {
        "issues": "https://github.com/CakeDC/cakephp-phppm/issues",
        "source": "https://github.com/CakeDC/cakephp-phppm"
    },
    "require": {
        "php": ">=5.6",
        "cakephp/cakephp": "^3.6",
        "php-pm/php-pm": "^1.0",
        "symfony/console": "^3.0",
        "symfony/debug": "^3.0",
        "symfony/process": "^2.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0",
        "cakephp/cakephp-codesniffer": "^2.0",
        "satooshi/php-coveralls": "dev-master"
    },
    "suggest": {
    },
    "autoload": {
        "psr-4": {
            "CakeDC\\PHPPM\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CakeDC\\PHPPM\\Test\\": "tests",
            "CakeDC\\PHPPM\\Test\\Fixture\\": "tests"
        }
    }
}
